			process_common.showWin();
		}
		if(me.addType=="newEdit"){
			var process_common = Ext.create('Wdbl.form.op.leaderInstructionsForm',{recordID:me.loadParam.id});
			process_common.showWin();
		}
		if(me.addType=="process"){
			var process_common = Ext.create('Wdbl.form.op.leaderInstructionsForm',{recordID:me.loadParam.id});
			process_common.showWin();
		}
	},
	// 转送单位
	f_transfer : function() {
		var me = this;
