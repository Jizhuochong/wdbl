var transfer = null;
var finish = null;
var noFinish = null;
Ext.define('Wdbl.form.CipherTelegraphEditForm', {
	extend : 'Wdbl.form.BaseEditForm',
	requires : ['Ext.form.FieldSet', 'Ext.form.FieldContainer', 'Ext.form.field.File', 'Ext.form.Label','Ext.ux.datetime.DateTimeField'],
	readPushBtn : null, 
	endingBtn : null, 
	modelId : null,
	/**
	 * 登记按钮组
	 * 
	 * @type Ext.grid.Panel
	 */
